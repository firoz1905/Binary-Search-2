class Solution:
    def findMin(self, nums: List[int]) -> int:
        self.low=0
        self.high=len(nums)-1
        
        while (self.low<=self.high):
            mid=self.low+ (self.high - self.low)//2
            if ((mid==0 or nums[mid+1]>nums[mid]) and (mid ==len(nums) or nums[mid-1]>nums[mid])):
                return nums[mid]
            if nums[mid]<nums[self.high]:
                self.high=mid-1
            else:
                self.low=mid+1
        return -1
